<%= form_with(model: Picture.new, local: true) do |form| %>
    <div class="picture_title">
      <%= form.label :title %>
      <%= form.text_field :title %>
    </div>
    <div class="picture_content">
      <%= form.label :content %>
      <%= form.text_field :content %>
    </div>
    <%= form.submit "Post" %>
  <% end %>